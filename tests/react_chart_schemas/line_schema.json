{
  "$id": "http://example.com/example.json",
  "type": "array",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "items": {
    "$id": "http://example.com/example.json/items",
    "type": "object",
    "properties": {
      "id": {
        "$id": "http://example.com/example.json/items/properties/id",
        "type": "string",
        "title": "The Id Schema ",
        "default": "",
        "examples": [
          "whisky"
        ]
      },
      "color": {
        "$id": "http://example.com/example.json/items/properties/color",
        "type": "string",
        "title": "The Color Schema ",
        "default": "",
        "examples": [
          "hsl(8, 70%, 50%)"
        ]
      },
      "data": {
        "$id": "http://example.com/example.json/items/properties/data",
        "type": "array",
        "items": {
          "$id": "http://example.com/example.json/items/properties/data/items",
          "type": "object",
          "properties": {
            "color": {
              "$id": "http://example.com/example.json/items/properties/data/items/properties/color",
              "type": "string",
              "title": "The Color Schema ",
              "default": "",
              "examples": [
                "hsl(249, 70%, 50%)"
              ]
            },
            "x": {
              "$id": "http://example.com/example.json/items/properties/data/items/properties/x",
              "type": "string",
              "title": "The X Schema ",
              "default": "",
              "examples": [
                "BA"
              ]
            },
            "y": {
              "$id": "http://example.com/example.json/items/properties/data/items/properties/y",
              "type": "integer",
              "title": "The Y Schema ",
              "default": 0,
              "examples": [
                2
              ]
            }
          }
        }
      }
    }
  }
}
